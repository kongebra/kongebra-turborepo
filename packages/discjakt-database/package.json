    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "dev": "tsup --watch",
    "format": "prisma format",
    "generate": "prisma generate",
    "lint": "eslint ./src --fix",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "studio": "prisma studio"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "prisma": "^4.4.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.3.0",
    "tsx": "^3.10.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0"
  }
}
